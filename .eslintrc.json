{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:jest/recommended",
        "standard",
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "jest",
        "prettier"
    ]
}
